<html>
<!-- TEST123 -->
   <head></head>
   <body>
   		<form name="calculator">
   			<table>
	            <tr>
	              <input type="text" name="display" id="display" disabled>
	            </tr>
	            <tr>
	              <td><input type= "button" name= "one" id= "1" value= "1" onclick = "calculator.display.value += '1'; displayCheck(this.id)"></td>
	              <td><input type="button" name="two" id = "2" value="2" onclick = "calculator.display.value += '2' ; displayCheck(this.id)"></td>
	              <td><input type= "button" name="three" id = "3" value= "3" onclick = "calculator.display.value += '3' ; displayCheck(this.id)"></td>
	              <td><input type= "button" name= "plus" id = "plus" value= "+" onclick = "adding()" ></td>
	           </tr>
	         	<tr>
	              <td><input type= "button" name= "four" id = "4" value= "4" onclick = "calculator.display.value += '4'; displayCheck(this.id)"></td>
	              <td><input type="button" name="five" id = "5" value="5" onclick = "calculator.display.value += '5'; displayCheck(this.id)"></td>
	              <td><input type= "button" name="six" id = "6" value= "6" onclick = "calculator.display.value += '6'; displayCheck(this.id)"></td>
	              <td><input type= "button" name= "minus" id = "minus" value= "-" onclick = "subtracting()"></td>
	           </tr> 
	           <tr>
	              <td><input type= "button" name= "seven" id = "7" value= "7" onclick = "calculator.display.value += '7'; displayCheck(this.id)"></td>
	              <td><input type="button" name="eight" id = "8" value="8" onclick = "calculator.display.value += '8'; displayCheck(this.id)"></td>
	              <td><input type= "button" name="nine" id = "9" value= "9" onclick = "calculator.display.value += '9'; displayCheck(this.id)"></td>
	              <td><input type= "button" name= "times" id = "times" value= "*" onclick = "multiplying()"></td>
	           </tr>
	           <tr>
	              <td><input type= "button" name= "clear" value= "C" onclick = "clearing()"></td>
	              <td><input type="button" name="zero" id = "0" value="0" onclick = "calculator.display.value += '0'; displayCheck(this.id)"></td>
	              <td><input type= "button" name="equals" value= "=" onclick = "equaling()"></td>
	              <td><input type= "button" name= "divide" id = "divide" value= "/" onclick = "dividing()"></td>
	           </tr>
	        </table>
   		</form>
   </body>
</html>